@using Localization.Resources.AbpUi
@using Microsoft.Extensions.Localization
@inherits SButton
@inject IStringLocalizer<AbpUiResource> L

@((RenderFragment)base.BuildRenderTree)

@code
{
    protected override RenderFragment GenerateSetting()
    {
        return __builder =>
        {
            <AddTemplate>
                @L["Submit"]
            </AddTemplate>
            @ChildContent
        };
    }
}
@namespace Secyud.Abp.MasaBlazorUi.Components

<MButton Color="@Color" Block="@Block" 
         Disabled="@IsDisabled" Loading="@IsLoading" 
         OnClick="@OnClickedHandler" Type="submit">
    @if ( ChildContent != null )
    {
        @ChildContent
    }
    else
    {
        @SaveString
    }
</MButton>
